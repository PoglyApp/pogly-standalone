{
  "interactiveLogin": true,
  "httpServer": "NettyWrapper",
  "tokenCallbacks": [
    {
      "issuerId": "default",
      "tokenExpiry": 3600,
      "requestMappings": [
        {
          "requestParam": "scope",
          "match": "*",
          "claims": {
            "sub": "test-user-123",
            "preferred_username": "testuser",
            "email": "testuser@example.com",
            "name": "Test User",
            "aud": ["pogly-test-client"],
            "iss": "http://mockoidc:8080/default"
          }
        }
      ]
    }
  ]
}
